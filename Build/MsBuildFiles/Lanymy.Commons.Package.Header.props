<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">


  <PropertyGroup>

    <TargetFrameworks>$(CurrentTargetFrameworks)</TargetFrameworks>

    <LanymyCommonFrameworkPackageReleaseNumbers>0.14.0</LanymyCommonFrameworkPackageReleaseNumbers>

    <CurrentReleaseNumbers>$(LanymyCommonFrameworkPackageReleaseNumbers)</CurrentReleaseNumbers>

  </PropertyGroup>


  <Import Project="Lanymy.Common.ExtensionFunctions.props" />
  <Import Project="Lanymy.Common.Instruments.props" />
  <Import Project="Lanymy.Common.Helpers.props" />


  <PropertyGroup>

    <Description>
      $(CurrentBuildProjectName)
      通用辅助类库. 序列化 ; 压缩 ; 数据流加密 ; 文件操作 ; 枚举扩展 ; 沙盒操作 ; 进程 ; 二维码 ; 反射 ; 版本 ; 流水号 ; 验证码 ; CMD命令行操作器 ; ffmpeg辅助类 ; 加密/解密 ;
    </Description>

    <PackageTags>common;helpers;extension;serialize;json;stream;zip;file;enum;sandbox;progress;QRCode;reflection;version;SerialNumber;VerificationCode;cmd;ffmpeg;encryption;</PackageTags>

  </PropertyGroup>


</Project>
