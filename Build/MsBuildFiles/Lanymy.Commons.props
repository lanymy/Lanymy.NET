<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!--<Import Project="Dependencies.props" />-->

  <!--MSBuild 保留属性表-->
  <!--https://docs.microsoft.com/zh-cn/visualstudio/msbuild/msbuild-reserved-and-well-known-properties?view=vs-2019-->
  <!--MSBuild 常见的项元数据-->
  <!--https://docs.microsoft.com/zh-cn/visualstudio/msbuild/msbuild-well-known-item-metadata?view=vs-2019-->
  <!--属性函数 列表-->
  <!--https://docs.microsoft.com/zh-cn/visualstudio/msbuild/property-functions?view=vs-2019-->

  <PropertyGroup>

    <CurrentTargetFrameworks>netcoreapp2.2;net48;netstandard2.0</CurrentTargetFrameworks>


    <LanymyCommonProjectName>Lanymy.Common</LanymyCommonProjectName>
    <LanymyCommonReleaseNumbers>0.4.4</LanymyCommonReleaseNumbers>

    <LanymyDtoModelsProjectName>Lanymy.DtoModels</LanymyDtoModelsProjectName>
    <LanymyDtoModelsReleaseNumbers>0.1.0</LanymyDtoModelsReleaseNumbers>

    <LanymyConfigProjectName>Lanymy.Config</LanymyConfigProjectName>
    <LanymyConfigReleaseNumbers>0.1.0</LanymyConfigReleaseNumbers>

    <LanymyCommonConsoleProjectName>Lanymy.Common.Console</LanymyCommonConsoleProjectName>
    <LanymyCommonConsoleReleaseNumbers>0.1.2</LanymyCommonConsoleReleaseNumbers>

    <CurrentBuildProjectName>$(MSBuildProjectName)</CurrentBuildProjectName>
    <CurrentReleaseNumbers>0.0.1</CurrentReleaseNumbers>

  </PropertyGroup>




  <PropertyGroup Condition=" $(CurrentBuildProjectName.EndsWith('$(LanymyCommonProjectName)')) ">

    <CurrentBuildProjectName>$(LanymyCommonProjectName)</CurrentBuildProjectName>
    <CurrentReleaseNumbers>$(LanymyCommonReleaseNumbers)</CurrentReleaseNumbers>
 
  </PropertyGroup>

  <PropertyGroup Condition=" $(CurrentBuildProjectName.EndsWith('$(LanymyDtoModelsProjectName)')) ">

    <CurrentBuildProjectName>$(LanymyDtoModelsProjectName)</CurrentBuildProjectName>
    <CurrentReleaseNumbers>$(LanymyDtoModelsReleaseNumbers)</CurrentReleaseNumbers>

  </PropertyGroup>

  <PropertyGroup Condition=" $(CurrentBuildProjectName.EndsWith('$(LanymyConfigProjectName)')) ">

    <CurrentBuildProjectName>$(LanymyConfigProjectName)</CurrentBuildProjectName>
    <CurrentReleaseNumbers>$(LanymyConfigReleaseNumbers)</CurrentReleaseNumbers>
 
  </PropertyGroup>

  <PropertyGroup Condition=" $(CurrentBuildProjectName.EndsWith('$(LanymyCommonConsoleProjectName)')) ">

    <CurrentBuildProjectName>$(LanymyCommonConsoleProjectName)</CurrentBuildProjectName>
    <CurrentReleaseNumbers>$(LanymyCommonConsoleReleaseNumbers)</CurrentReleaseNumbers>
    
  </PropertyGroup>





  <PropertyGroup>


    <AssemblyName>$(CurrentBuildProjectName)</AssemblyName>
    <RootNamespace>$(CurrentBuildProjectName)</RootNamespace>


    <AssemblyVersion>$(CurrentReleaseNumbers)</AssemblyVersion>

    <FileVersion>$(CurrentReleaseNumbers)</FileVersion>

    <Version>$(CurrentReleaseNumbers)</Version>





    <PackageId>$(CurrentBuildProjectName)</PackageId>

    <Authors>Lanymy</Authors>

    <Company>Copyright © Lanymy 2019</Company>

    <Product>Lanymy.NET</Product>

    <PackageIcon>icon.png</PackageIcon>

    <PackageIconUrl>https://raw.githubusercontent.com/lanymy/Lanymy.SharedBinary.Public/master/src/imgs/icon.ico</PackageIconUrl>

    <PackageLicenseFile>LICENSE</PackageLicenseFile>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

  </PropertyGroup>



</Project>
